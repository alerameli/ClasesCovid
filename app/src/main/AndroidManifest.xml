<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alejandroramirez.covidalert">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Clases Covid"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CovidAlert">
        <activity
            android:name=".AgregarClase"
            android:exported="true" />
        <activity
            android:name=".PaginaClasesPasasdasAlumno"
            android:exported="true" />
        <activity
            android:name=".PaginaClasesFuturasAlumno"
            android:exported="true" />
        <activity
            android:name=".VerInfoClaseAlumno"
            android:exported="true" />
        <activity
            android:name=".SignUpActivity"
            android:exported="true" />
        <activity
            android:name=".PaginaInicioAlumno"
            android:exported="true" />
        <activity
            android:name=".PaginaInicioMaestro"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>